entitymodel baseVehicle /* define a property model for a vehicle */

    property speed = 100
    property protection = 50

end


entity Lamborghini of type vehicle
    applymodel baseVehicle /* apply all propertys from baseVehicle model */

    property speed = 150

end

entity FordFiesta of type vehicle 
    applymodel baseVehicle
end

entity MG11 of type weapon 

    property power = 10

end